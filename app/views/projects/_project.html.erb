<div id="<%= dom_id project %>" class="flex items-center justify-between gap-x-6 py-3 md:py-5">
  <div class="min-w-0">
    <%= link_to project do %>
      <div class="flex items-start gap-x-3">
        <p class="font-semibold leading-6 text-gray-900 text-base">
          <%= project.name %>
        </p>
      </div>
      <div class="mt-1 flex items-center gap-x-2 text-xs leading-5 text-gray-500">
        Created by <%= project.user.email %>
      </div>
    <% end %>

    <div class="flex flex-none items-center gap-x-4">

    </div>
  </div>
</div>
